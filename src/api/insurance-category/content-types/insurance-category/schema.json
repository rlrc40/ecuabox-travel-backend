{
  "kind": "collectionType",
  "collectionName": "insurance_categories",
  "info": {
    "singularName": "insurance-category",
    "pluralName": "insurance-categories",
    "displayName": "InsuranceCategory"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "strech_of_days": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::strech-of-days.strech-of-days",
      "mappedBy": "insurance_category"
    },
    "insurance": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::insurance.insurance",
      "mappedBy": "insurance_category"
    }
  }
}
